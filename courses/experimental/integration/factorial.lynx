window = new group
    name:"group"
    childElement1: new text
        name:"txt"
        x:20
        y:20
        innerText:new get
            jsPrimitive:{get}
            attribute:equalTo
            rootObject: new object
                name:"apply"
                equalTo:apply.function.result
                function: new dbSearch
                    jsPrimitive:{dbSearch}
